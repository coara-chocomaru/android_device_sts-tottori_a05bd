/data                        u:object_r:data_file:s0     dir
/data(/.*)                   u:object_r:data_file:s0     file
/metadata                    u:object_r:metadata_file:s0 dir
/metadata(/.*)               u:object_r:metadata_file:s0 file
/dev/block/platform/bootdevice/by-name/userdata
                             u:object_r:cryptfs_device:s0 blk_file
/dev/block/platform/bootdevice/by-name/metadata
                             u:object_r:cryptfs_device:s0 blk_file
/dev/block/mapper(/.*)?      u:object_r:device_mapper:s0 chr_file
/dev/block/dm-[0-9]+         u:object_r:cryptfs_device:s0
/dev/block/loop-control      u:object_r:loop_device:s0   chr_file
/dev/block/loop.*            u:object_r:loop_device:s0   blk_file
/tmp                         u:object_r:tmpfs:s0          dir
/tmp(/.*)                    u:object_r:tmpfs:s0          file
/mnt/media_rw(/.*)?          u:object_r:sdcardfs:s0       dir
/mnt/media_rw(/.*)           u:object_r:sdcardfs:s0       file
